<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>fizzbuzz assignment</title>
</head>
<body>
    <script>
        for(var i=1;i<=100;i++){
            if(i%15==0){
                document.write("FizzBuzz");
                document.write("<br>");
            }
            else if(i%3==0){
                document.write("Fizz");
                document.write("<br>");
            }
            else if(i%5==0){
                document.write("Buzz");
                document.write("<br>");
            }
            else{
                document.write(i);
                document.write("<br>");
            }
        }
        
    </script>
    
</body>
</html>
